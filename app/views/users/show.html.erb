<div class="user-page-container">
  <%= render "shared/user_details", user: @user %>
    <div class="user-bio-container">
     <strong class="bio-title">Bio: </strong> <br>
     <p><%= @user.bio %></p>
    </div>
    <div class="user-posts-container">
    <% @user.most_recent_posts.each do |post| %>
        <%= link_to user_post_path(user_id:@user.id, id: post.id), class:'nav-link' do%>
      <%= render "shared/post_dets", post: post %>
        <%end%>
    <%end%>
    </div>
    <%= link_to 'See all posts', user_posts_path(@user), class: "post-btn"%>
</div>
